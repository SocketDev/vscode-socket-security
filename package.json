{
    "name": "codelens-sample",
    "displayName": "Socket Security",
    "description": "Samples for VS Code's CodeLens API",
    "version": "0.0.1",
    "publisher": "ms-vscode",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-extension-samples"
    },
    "engines": {
        "vscode": "^1.26.0"
    },
    "activationEvents": [
        "workspaceContains:./socket.yml"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
        ],
        "configuration": {
            "properties": {
                "socket-dev.socketSecurityAPIKey": {
                    "type": "string",
                    "description": "API key for socket.dev API"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint \"src/**/*.ts\"",
        "watch": "tsc -watch -p ./"
    },
    "bundledDependencies": [
        "@socketsecurity/cli"
    ],
    "dependencies": {
        "@socketsecurity/cli": "*",
        "json-to-ast": "^2.1.0",
        "yaml": "^2.1.3"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.26.0",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "typescript": "^4.8.4"
    }
}
